.bottombBtn {
  padding: 0 20rpx;
  padding-bottom: 30rpx;
  margin-top: 20rpx;
  width: 710rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}
.bottombBtn .btn {
  padding: 0;
  margin-top: 24rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 33rpx;
  font-weight: 600;
  color: white;
  text-align: center;
  border-radius: 18rpx;
}
.bottombBtn .left {
  flex: 5;
  border: 3rpx solid #213EB3;
  height: 80rpx;
  border-radius: 10rpx;
  font-size: 33rpx;
  font-weight: 600;
  line-height: 80rpx;
  text-align: center;
  margin-right: 15rpx;
}
.bottombBtn .right {
  flex: 5;
  border: 3rpx solid #213EB3;
  height: 80rpx;
  border-radius: 10rpx;
  font-size: 33rpx;
  font-weight: 600;
  line-height: 80rpx;
  text-align: center;
}
